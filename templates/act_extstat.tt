<html>
<!-- $Id: act_extstat.tt,v 1.2 2011-08-23 17:54:27 gosha Exp $ -->
<head>
	<title>Virtual PBX - Extension extended stats - Ac:[% USER_CREDS.ACCESS_CODE | html %]</title>

	<link rel="stylesheet" type="text/css" media="all" href="/xvb/css/calendar-blue.css"  />

	[% IF USER_CREDS.CSS_HREF eq '' %]
	<link rel="stylesheet" type="text/css" href="/xvb/xvb.css" />
	[% ELSE %]
	<link rel="stylesheet" type="text/css" href="[% USER_CREDS.CSS_HREF %]" />
	[% END %]

	<script language="JavaScript" src="/xvb/js/xvb.js"></script>
	<script type="text/javascript" src="/xvb/js/jquery/jquery.min.js"></script>
	<script type="text/javascript" src="/xvb/js/jquery/calendar/jquery.dynDateTime.min.js"></script>
	<script type="text/javascript" src="/xvb/js/jquery/calendar/lang/calendar-en.min.js"></script>
	<script type="text/javascript" src="https://www.google.com/jsapi"></script>
	
	<script language="javascript">

		js_data = [% JS_DATA %];

		[% IF VBOX_PREF.TYPE eq '13' %]
		google.load("visualization", "1", {packages:["imagebarchart","corechart","table"]});
		google.setOnLoadCallback(drawAllChart);
		function drawAllChart() {
			ExtStatdrawChart1('STATUS','All calls / disposition');
			ExtStatdrawChart1('CAUSE','All calls / cause code');
			ExtStatdrawChart1('DURATION','Answered calls / duration (minutes)');
			ExtStatdrawChart1('PN','Answered calls / agent');
			ExtStatdrawChart1('Q_NUM_OUT','Unanswered calls / out queue position');
			ExtStatdrawChart2('Q_NUM',['NOANSWER'],'NOANSWER','Unanswered calls / start queue position');
			ExtStatdrawChart2('Q_NUM',['ANSWER'],'ANSWER','Answered calls / start queue position');
			ExtStatdrawChart2('Q_NUM',['ANSWER','NOANSWER'],'ALL','All calls / start queue position');
			ExtStatdrawChart2('TERM',['NOANSWER'],'NOANSWER','Unanswered calls / disconnect originator');
			ExtStatdrawChart2('TERM',['ANSWER'],'ANSWER','Answered calls / disconnect originator');
			ExtStatdrawChart2('RING_TIME',['NOANSWER'],'NOANSWER','Unanswered calls / wait time (sec)');
			ExtStatdrawChart2('RING_TIME',['ANSWER'],'ANSWER','Answered calls / wait time (sec)');
		}
		[% END %]
		
		var period='[% CDR_PERIOD %]';
		var group_by = '';
		if ( period == '%Y-%m-%d' ) {
			period = 'day';
			group_by = 'd';
		} else if ( period == '%W' ) {
			period = 'day of week';
			group_by = 'dw';
		} else if ( period == '%V' ) {
			period = 'week';
			group_by = 'w';
		} else if ( period == '%Y-%m' ) {
			period = 'month';
			group_by = 'm';
		} else if ( period == '%H' ) {
			period = 'hour of day';
			group_by = 'hd';
		} else if ( period == '%Y-%m-%d %H' ) {
			period = 'hour';
			group_by = 'h';
		} else if ( period == '%Y-%m-%d %H:%i' ) {
			period = 'minute';
			group_by = 'min';
		} else {
			period = 'group by';
		}
	</script>
</head>

<body onLoad="XVBInit()">
	<center>
	<p>
	<table width='95%' class='logo'>
		<tr>
			<td align='left' width='20%'>
			[% IF USER_CREDS.LOGO_HREF eq '' %]
				<img border='0' src='/xvb/images/logo-small.png' alt='Virtual PBX' />
			[% ELSE %]
				<img border='0' src='[% USER_CREDS.LOGO_HREF %]' alt='Virtual PBX' />
			[% END %]
			</td>
			<td align='center'><h2>Extension #[% VBOX_PREF.EXT_NUMBER | html %] extended stats</h2><h3>( [% CDR_HEADER %] 
			<script language='JavaScript'>
				document.write( ', group by : ' + period );
			</script>
			)</h3></td>
			<td align='right' width='20%'><a href=?action=logout>logout</a></td>
		</tr>
	</table>
	</p>

	<p>
	<table width='95%' class='top_hr'>
		<tr align='left'>
			<td>
				<a href='?action=msg_list&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>View messages</a>
				&nbsp;&nbsp;|&nbsp;&nbsp;
				<a href='?action=vb_view&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>Extension detail</a>
				&nbsp;&nbsp;|&nbsp;&nbsp;
				<a href='?action=ext_stat&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>Stats</a>
			</td>
		</tr>
	</table>
	</p>
	<p>
	<table width='95%' class='bottom_hr'>
		<form method="get" action="[% CGI_URL %]" onSubmit='return checkChanges(this)'>
		<tr align='right'>
			<td width='100%'>
				From date:
				<script type="text/javascript">
					jQuery(document).ready(function() {
						jQuery("#from_time").dynDateTime({
							showsTime: true,
							ifFormat: "%Y/%m/%d %H:%M",
							align: "TL",
							electric: false,
							singleClick: false,
							button: ".next()"
						});
				});
				</script>
				<input size='16' type='text' name='from_time' value='[% FROM_TIME | html %]' id='from_time'>
				<button type="button">...</button>
				&nbsp;
				To date:
				<script type="text/javascript">
					jQuery(document).ready(function() {
						jQuery("#to_time").dynDateTime({
							showsTime: true,
							ifFormat: "%Y/%m/%d %H:%M",
							align: "TL",
							electric: false,
							singleClick: false,
							button: ".next()"
						});
				});
				</script>
				<input size='16' type='text' name='to_time' value='[% TO_TIME | html %]' id='to_time'>
				<button type="button">...</button>
				&nbsp;
				Caller ID:
				<input size='10' type='text' name='caller_id' value='[% CDR_CALLER_ID | html %]'>
				<select name='chart_group_period'>
				<script language='Javascript'>
					document.write( "<option value='"+ group_by +"'>"+ period +"</option>" );
				</script>
				<noscript>
				<option value=''>group by</option>
				</noscript>
				<option value='min'>minute</option><option value='h'>hour</option><option value='hd'>hour of day</option><option value='d'>day</option><option value='dw'>day of week</option><option value='w'>week</option><option value='m'>month</option></select>
				&nbsp;
				<input name='action' value='extended_stat' type='hidden'>
				<input name='uniq' value='[% USER_CREDS.UNIQ %]' type='hidden'>
				<input name='id' value='[% VBOX_PREF.ID %]' type='hidden'>
				<input value='Search' type='submit'>
				&nbsp;&nbsp;
			</td>
		</tr>
		</form>
	</table>
	</p>

	<p>
	[% IF VBOX_PREF.TYPE eq '13' %]
	<table width='95%' border='0' align='center'>
		<tr><td colspan='2' align='center'><br><h4>Calls</h4></td></tr>
		<tr>
			<td width='60%'>
				<div id="chart_STATUS"></div>
			</td>
			<td width='40%'>
				<div id="chart_STATUS_PRC"></div>
			</td>
		</tr>
		
		<tr><td colspan='2' align='center'><br><h4>Wait time</h4></td></tr>
		<tr><td colspan='2'><br><br></td></tr>
		<tr>
			<td>
				<div id="chart_RING_TIME_ANSWER"></div>
			</td>
			<td>
				<div id="chart_RING_TIME_ANSWER_PRC"></div>
			</td>
		</tr>
		
		<tr><td colspan='2'><br><br></td></tr>
		<tr>
			<td>
				<div id="chart_RING_TIME_NOANSWER"></div>
			</td>
			<td>
				<div id="chart_RING_TIME_NOANSWER_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2' align='center'><br><h4>Agents</h4></td></tr>
		<tr>
			<td>
				<div id="chart_PN"></div>
			</td>
			<td>
				<div id="chart_PN_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2' align='center'><br><h4>Position in Queue</h4></td></tr>
		<tr>
			<td>
				<div id="chart_Q_NUM_OUT"></div>
			</td>
			<td>
				<div id="chart_Q_NUM_OUT_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2'><br><br></td></tr>
		<tr>
			<td>
				<div id="chart_Q_NUM_ALL"></div>
			</td>
			<td>
				<div id="chart_Q_NUM_ALL_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2'><br><br></td></tr>
		<tr>
			<td>
				<div id="chart_Q_NUM_NOANSWER"></div>
			</td>
			<td>
				<div id="chart_Q_NUM_NOANSWER_PRC"></div>
			</td>
		</tr>
		
		<tr><td colspan='2'><br><br></td></tr>
		<tr>
			<td>
				<div id="chart_Q_NUM_ANSWER"></div>
			</td>
			<td>
				<div id="chart_Q_NUM_ANSWER_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2' align='center'><br><h4>Disconnect originator</h4></td></tr>
		<tr>
			<td>
				<div id="chart_TERM_NOANSWER"></div>
			</td>
			<td>
				<div id="chart_TERM_NOANSWER_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2'><br><br></td></tr>
		<tr>
			<td>
				<div id="chart_TERM_ANSWER"></div>
			</td>
			<td>
				<div id="chart_TERM_ANSWER_PRC"></div>
			</td>
		</tr>
		
		<tr><td colspan='2' align='center'><br><h4>Call duration</h4></td></tr>
		<tr>
			<td>
				<div id="chart_DURATION"></div>
			</td>
			<td>
				<div id="chart_DURATION_PRC"></div>
			</td>
		</tr>

		<tr><td colspan='2' align='center'><br><h4>Tech info</h4></td></tr>
		<tr>
			<td>
				<div id="chart_CAUSE"></div>
			</td>
			<td>
				<div id="chart_CAUSE_PRC"></div>
			</td>
		</tr>
	</table>
	[% END %]
	</p>

	<p>
	<table width='95%' class='bottom_hr'>
		<form method="get" action="[% CGI_URL %]" onSubmit='return checkChanges(this)'>
		<tr align='right'>
			<td width='100%'>
				From date:
				<script type="text/javascript">
					jQuery(document).ready(function() {
						jQuery("#from_time").dynDateTime({
							showsTime: true,
							ifFormat: "%Y/%m/%d %H:%M",
							align: "TL",
							electric: false,
							singleClick: false,
							button: ".next()"
						});
				});
				</script>
				<input size='16' type='text' name='from_time' value='[% FROM_TIME | html %]' id='from_time'>
				<button type="button">...</button>
				&nbsp;
				To date:
				<script type="text/javascript">
					jQuery(document).ready(function() {
						jQuery("#to_time").dynDateTime({
							showsTime: true,
							ifFormat: "%Y/%m/%d %H:%M",
							align: "TL",
							electric: false,
							singleClick: false,
							button: ".next()"
						});
				});
				</script>
				<input size='16' type='text' name='to_time' value='[% TO_TIME | html %]' id='to_time'>
				<button type="button">...</button>
				&nbsp;
				Caller ID:
				<input size='10' type='text' name='caller_id' value='[% CDR_CALLER_ID | html %]'>
				<select name='chart_group_period'>
				<script language='Javascript'>
					document.write( "<option value='"+ group_by +"'>"+ period +"</option>" );
				</script>
				<noscript>
				<option value=''>group by</option>
				</noscript>
				<option value='min'>minute</option><option value='h'>hour</option><option value='hd'>hour of day</option><option value='d'>day</option><option value='dw'>day of week</option><option value='w'>week</option><option value='m'>month</option></select>
				&nbsp;
				<input name='action' value='extended_stat' type='hidden'>
				<input name='uniq' value='[% USER_CREDS.UNIQ %]' type='hidden'>
				<input name='id' value='[% VBOX_PREF.ID %]' type='hidden'>
				<input value='Search' type='submit'>
				&nbsp;&nbsp;
			</td>
		</tr>
		</form>
	</table>
	</p>
	
	</center>
	<div id='shadow'><div id='loading'></div></div><div id='center'></div>
</body>
</html>

