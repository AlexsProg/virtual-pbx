<html>
<!-- $Id: peer_list.tt,v 1.3 2014/09/07 18:30:40 gosha Exp $ -->
<head>
	<title>Virtual PBX - SIP Peers</title>
	[% IF USER_CREDS.CSS_HREF eq '' %]
	<link rel="stylesheet" type="text/css" href="/xvb/xvb.css" />
	[% ELSE %]
	<link rel="stylesheet" type="text/css" href="[% USER_CREDS.CSS_HREF %]" />
	[% END %]
	<script language="JavaScript" src="/xvb/js/xvb.js"></script>
</head>

<body onLoad="XVBInit()">
	<table width='95%' class='logo'>
		<tr align='left'>
			<td width='10%'>
				<img border='0' src='/xvb/images/logo-small.png' alt='Virtual PBX' />
			</td>
			<td valign='middle'>
				[% INCLUDE main_menu.tt-inc %]
				[% INCLUDE main_menu.tt-inc-custom %]
			</td>
			<td align='right'>
				<a href='/xvb/XVB-AI.pdf?a=msg_download'><img border='0' src='/xvb/images/help.png' alt='about' /></a>
				[% INCLUDE version.tt %]
			<td>
		</tr>
	</table>
	<center>
	
	<h1>SIP peers</h1>

	<table width='100%'>
		<tr>
			<td align='right'>
				<form method='post'>
					<input size='15' name='host' type='text' placeholder='Proxy' value='[% HOST | html %]'>
					<input size='11' name='authname' type='text' placeholder='Peer ID' value='[% AUTHNAME | html %]'>
					<input type='hidden' value='peer_list' name='action'>
					<input value='search' type='submit'>
				</form>
			</td>
		</tr>
	</table>
	
	<p>
	<table width='95%' id='d-tbl'>
		<tr>
			<th>Access code</th>
			<th>Peer ID</th>
			<th>Proto</th>
			<th>Proxy</th>
			<th>Port</th>
			<th>Domain</th>
			<th>User</th>
			<th>Password</th>
			<th>CallerID</th>
			<th>DTMF mode</th>
			<th>Need regs</th>
			<th>&nbsp;</th>
		</tr>
		
		[% IF MSG_PREV ne '' %]
		<tr>
			<td align='left' colspan='12'>
				<a title='previous page' href='?action=peer_list&host=[% HOST | html %]&from=[% MSG_PREV %]&authname=[% NAME | html %]'><img border='0'  src='/xvb/images/msg_prev.png' alt='[% MSG_PREV %] &lt;=' /></a>
			</td>
		</tr>
		[% END %]
		
		[% FOREACH p = SIPDEV %]
		<tr align='left' class='nocolor'>
			<td><a title='edit user settings' href='?ac=[% p.ACCESS_CODE %]&action=user_info'>[% p.ACCESS_CODE %]</a></td>
			<td>[% p.name %]</td>
			<td>[% p.transport | html %]</td>
			<td><span title='[% p.DESCRIPTION | html %]'>[% p.host | html %]</span></td>
			<td>[% p.port | html %]</td>
			<td>[% p.fromdomain | html %]</td>
			<td>[% p.username | html %]</td>
			<td>[% p.secret | html %]</td>
			<td>[% p.fromuser | html %]</td>
			<td>[% p.dtmfmode | html %]</td>
			<td>
			[% IF p.NEED_REG eq '1' %]
				yes
			[% ELSE %]
				no
			[% END %]
			</td>
			<td>
			[% IF p.REG_DATA %]
				<img 
				[% IF p.REG_DATA.state eq 'Registered' %]
					src='/xvb/images/ball_green.png'
				[% ELSE %]
					src='/xvb/images/ball_red.png'
				[% END %]
				title='[% p.REG_DATA.state | html %]' alt='[% p.REG_DATA.state | html %]' />
			[% END %]
			</td>
		</tr>
		[% END %]
	
		<tr>
		[% IF MSG_NEXT ne '' %]
			<td align='right' colspan='12'>
				<a title='previous page' href='?action=peer_list&host=[% HOST | html %]&from=[% MSG_NEXT %]&authname=[% NAME | html %]'><img border='0'  src='/xvb/images/msg_next.png' alt='[% MSG_PREV %] &gt;=' /></a>
			</td>
		[% ELSE %]
			<td colspan='12'>&nbsp;</td>
		[% END %]
		</tr>
	</table>
	</p>

	<script language='JavaScript'>listcolorer( 'd-tbl' );</script>

	</center>
	<div id='shadow'><div id='loading'></div></div><div id='center'></div>
	</body>
</html>
