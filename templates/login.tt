<html>
<!-- $Id: login.tt,v 1.15 2010-08-05 20:51:19 gosha Exp $ -->
<head>
	<title>Virtual PBX - login</title>
	<link rel="stylesheet" type="text/css" href="/xvb/xvb.css" />
	<script language="JavaScript" src="/xvb/js/xvb.js"></script>
</head>

<body onLoad="XVBInit()">
	<center>
	<p>
	<table width='95%' class='logo'>
		<tr>
			<td align='left' width='20%'>
				<img border='0' src='/xvb/images/logo-small.png' alt='Virtual PBX' />
			</td>
			<td align='center'><h2>Welcome to VirtualPBX</h2></td>
			<td align='right' width='20%'>
				<a href='/xvb/XVB.pdf?a=msg_download'><img border='0' src='/xvb/images/help.png' alt='about' /></a>
			</td>
		</tr>
	</table>
	</p>

	<p>
	<table width='95%'class='bottom_hr'>
		<tr>
			<td>&nbsp;&nbsp;</td>
		</tr>
	</table>
	</p>

	<p align='center'>
	<table width='50%'>
		<form method='post'>
			<tr>
				<td class='ext_pn'>Phone number: </td>
				<td class='ext_pv'><input name='phone' type='text'></td>
			</tr>
			<tr>
				<td class='ext_pn'>Access code :</td>
				<td class='ext_pv'><input name='ac' type='text'></td>
			</tr>
			<tr>
				<td class='ext_pn'>Extension :</td>
				<td class='ext_pv'><input name='ext_id' type='text'></td>
			</tr>
			<tr>
				<td class='ext_pn'>Pin code:</td>
				<td class='ext_pv'><input name='pw' type='password'></td>
			</tr>
			<tr>
				<td colspan='2'>&nbsp;</td>
			</tr>
			<tr>
				<td colspan='2' align='center'>
					<input name='referer' type='hidden' value='[% REFERER | html %]'>
					<input name='action' type='hidden' value='start'>
					<input type='submit' value='&nbsp;&nbsp;&nbsp;LOGIN&nbsp;&nbsp;&nbsp;'>
				</td>
			</tr>
		</form>
	</table>
	</p>

	<p>
	<table width='95%' class='bottom_hr'>
		<tr align='center'>
			<td>&nbsp;
			[% IF ERROR_STR ne '' %]
				<span class='error'>[% ERROR_STR | html %]</span>
			[% END %]
			</td>
		</tr>
	</table>
	</p>
	</center>
	<div id='shadow'><div id='loading'></div></div><div id='center'></div>
</body>
</html>
