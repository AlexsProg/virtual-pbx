							<td><input size='7' type='text'  name='dayofweek' value='*'></td>
							<td><input size='7' type='text'  name='dayofmonth' value='*'></td>
							<td><input size='5' type='text'  name='month' value='*'></td>
							<td><input size='9' type='text'  name='year' value='*'></td>
							<td><input size='11' type='text'  name='redirect_to' value='0'></td>
							<td align='center'>
								<input type='submit' value='add'>
							</td>
							</form>
						</tr>
					</table>
				</td>
			</tr>
			[% END %]

			[% IF VBOX_PREF.TYPE eq '18' %]
			<tr><td colspan='3'>&nbsp;</td></tr>
			<tr>
				<td colspan='2' align='left'><h4>CallBlast data ( [% MSG_FROM | html %] .. [% MSG_TO | html %] ) :</h4></td>
				<td align='right'>
					<a title='get excel table' href='?action=vb_view&id=[% VBOX_PREF.ID %]&from=all&uniq=[% USER_CREDS.UNIQ %]&need_excel=1'"><img border='0' src='/xvb/images/excel.png' alt='excel' /></a>
					&nbsp;&nbsp;&nbsp;
					<a href='?action=callblast_del&id=[% VBOX_PREF.ID %]&data_id=all&uniq=[% USER_CREDS.UNIQ %]' onclick="return DelConfirm('en')" title='delete ALL callblast items'><img border='0' src='/xvb/images/msg_delete.png' alt='del all items' /></a>
				</td>
			</tr>
			<tr>
				<td colspan='3' align='center'>
				<table width='95%' class='addon_data' id='d-tbl'>
					<tr class='list_data'>
						<th>Name</th>
						<th>Phone Number(s)</th>
						<th>User vars</th>
						<th>Status</th>
						<th>Attempt</th>
						<th>Handle time</th>
						<th>Actions</th>
					</tr>
					[% IF MSG_PREV ne '' %]
					<tr>
						<td align='left' colspan='7'>
							<a title='previous page' href='?action=vb_view&id=[% VBOX_PREF.ID %]&from=[% MSG_PREV %]&uniq=[% USER_CREDS.UNIQ %]'><img border='0'  src='/xvb/images/msg_prev.png' alt='[% MSG_PREV %] &lt;=' /></a>
						</td>
					</tr>
					[% END %]
					[% FOREACH param = VB_DATA %]
					<tr align='center' class='backlight' id='d-tr-[% param.DATA_ID %]'>
						<form method="post" action="[% CGI_URL %]" onSubmit='return xmlhttpPost(this,1)'>
						<input type='hidden' name='action' value='callblast_update'>
						<input type='hidden' name='id' value='[% VBOX_PREF.ID %]'>
						<input type='hidden' name='data_id' value='[% param.DATA_ID %]'>
						<input type='hidden' name='uniq' value='[% USER_CREDS.UNIQ %]'>
						<td><input class='in_t0' size='20' type='text' name='name' value='[% param.NAME | html %]'></td>
						<td><input class='in_t0' size='35' type='text' name='pn' value='[% param.PHONE_NUMBER | html %]'></td>
						<td><input class='in_t0' size='35' type='text' name='uv' value='[% param.USER_VARS | html %]'></td>
						<td>
							[% IF param.CALL_STATUS contain 'success' %]
								<a class='success' href='?action=act_list&call_id=[% param.CALL_ID %]&id=[% ROOT_EXT_ID %]&uniq=[% USER_CREDS.UNIQ %]'>[% param.CALL_STATUS | html %]</a>
							[% ELSIF param.CALL_STATUS eq 'wait' %]
								wait	
							[% ELSIF param.CALL_STATUS %]
								<span class='error'>[% param.CALL_STATUS | html %]</span>
							[% ELSE %]
								unknown
							[% END %]
						</td>
						<td>[% param.ATTEMPT %]</td>
						<td>
							[% param.PROCESS_DATE | html %]
						</td>
						<td align='center'>
							<table>
								<tr>
									<td>
										<a href='?action=callblast_del&id=[% VBOX_PREF.ID %]&data_id=[% param.DATA_ID | html %]&uniq=[% USER_CREDS.UNIQ %]' onclick="return xmlhttpDel(this,'en','d-tr-[% param.DATA_ID %]')" title='delete callblast item'><img border='0' src='/xvb/images/msg_delete.png' alt='del' /></a>
									</td>
									<td>
										<span class='in_t' id='in_t_id-[% param.DATA_ID %]'>
										<script language='JavaScript'>
											writeUpdateLinks( 'save changes' );
										</script>
										<noscript><input value='change' type='submit'></noscript>
										</span>
									</td>
								</tr>
							</table>
						</td>
						</form>
					</tr>
					[% END %]
					[% IF MSG_NEXT ne '' %]
					<tr>
						<td align='right' colspan='7'>
							<a title='next page' href='?action=vb_view&id=[% VBOX_PREF.ID %]&from=[% MSG_NEXT %]&uniq=[% USER_CREDS.UNIQ %]'><img border='0'  src='/xvb/images/msg_next.png' alt='[% MSG_PREV %] &gt;=' /></a>
						</td>
					</tr>
					[% END %]
					<tr align='center' class='nocolor'>
						<form method="post" action="[% CGI_URL %]" onSubmit='return checkChanges(this)'>
						<input type='hidden' name='action' value='callblast_add'>
						<input type='hidden' name='id' value='[% VBOX_PREF.ID %]'>
						<input type='hidden' name='uniq' value='[% USER_CREDS.UNIQ %]'>
						<td><input type='text' name='name' value='' size='20'></td>
						<td><input size='35' type='text' name='pn' value=''></td>
						<td><input size='35' type='text' name='uv' value=''></td>
						<td colspan='3'>&nbsp;</td>
						<td align='center'>
							<input type='submit' value='add'>
						</td>
						</form>
					</tr>
					<tr><td colspan='7'>&nbsp;</td></tr>
					<tr id='csv-import'>
						<form enctype="multipart/form-data" method="post" action="https://[% USER_CREDS.DOWNLOAD_IP %][% CGI_URL %]" onSubmit='return checkChanges(this)'>
						<td colspan='3'>&nbsp;</td>
						<td colspan='4' align='right'>
							<input type='file' name='config_file'>
							<input type='hidden' name='id' value='[% VBOX_PREF.ID %]'>
							<input type='hidden' name='action' value='ext_restore'>
							<input type='hidden' name='uniq' value='[% USER_CREDS.UNIQ %]'>
							<input name='is_excel' type='submit' value='Append data from CSV'>
						</td>
						</form>
					</tr>
					</table>
				</td>
			</tr>
			[% END %]

			[% IF VBOX_PREF.TYPE eq '21' %]
			<tr><td colspan='3'>&nbsp;</td></tr>
			<tr>
				<td colspan='2' align='left'><h4>Goto If data:</h4></td>
				<td align='right'>
					<a href='?action=gotoif_del&id=[% VBOX_PREF.ID %]&data_id=all&uniq=[% USER_CREDS.UNIQ %]' onclick="return DelConfirm('en')" title='delete ALL gotoif items'><img border='0' src='/xvb/images/msg_delete.png' alt='del all items' /></a>
				</td>
			</tr>
			<tr>
				<td colspan='3' align='center'>
					<table width='90%' class='addon_data' id='d-tbl'>
						<tr class='list_data'>
							<th>Priority</th>
							<th>Function</th>
							<th>Variable</th>
							<th>Condition</th>
							<th>Value</th>
							<th>Extension Number</th>
							<th>Actions</th>
						</tr>
						[% FOREACH param = VB_DATA %]
						<tr align='center' class='backlight' id='d-tr-[% param.DATA_ID %]'>
							<form method="post" action="[% CGI_URL %]" onSubmit='return xmlhttpPost(this,1)'>
							<input type='hidden' name='action' value='gotoif_update'>
							<input type='hidden' name='id' value='[% VBOX_PREF.ID %]'>
							<input type='hidden' name='data_id' value='[% param.DATA_ID %]'>
							<input type='hidden' name='uniq' value='[% USER_CREDS.UNIQ %]'>
							<td><input class='in_t0' size='5'  name='priority' type='text' value='[% param.PRIORITY | html %]'></td>
							<td>
								<select class='in_t0'  name="func">
									<option selected value="[% param.FUNC | html %]" selected>[% param.FUNC | html %]</option>
									<option value="value">value</option>
									<option value="strlen">strlen</option>
								</select> 
							</td>
							<td><input class='in_t0' size='15' name='var_name' type='text' value='[% param.VAR_NAME | html %]'></td>
							<td>
								<select class='in_t0' name="cond">
									<option selected value="[% param.COND | html %]" selected>[% param.COND | html %]</option>
									<option value="<">&lt;</option>
									<option value=">">&gt;</option>
									<option value="<=">&lt;=</option>
									<option value=">=">&gt;=</option>
									<option value="==">==</option>
									<option value="!-">!-</option>
									<option value="contain">contain</option>
									<option value="regexp">regexp</option>
								</select> 
							</td>
							<td><input class='in_t0' size='15' name='var_value' type='text' value='[% param.VAR_VALUE | html %]'></td>
							<td><input class='in_t0' size='11'  name='redirect_to' type='text' value='[% param.REDIRECT_TO | html %]'></td>
							<td align='left'>
								<table>
									<tr>
										<td>
											<a href='?action=gotoif_del&id=[% VBOX_PREF.ID %]&data_id=[% param.DATA_ID | html %]&uniq=[% USER_CREDS.UNIQ %]' onclick="return xmlhttpDel(this,'en','d-tr-[% param.DATA_ID %]')" title='delete gotoif item'><img border='0' src='/xvb/images/msg_delete.png' alt='del' /></a>
										</td>
										<td>
											<span class='in_t' id='in_t_id-[% param.DATA_ID %]'>
											<script language='JavaScript'>
												writeUpdateLinks( 'save changes' );
											</script>
											<noscript><input value='change' type='submit'></noscript>
											</span>
										</td>
										<td>
											[% FOREACH vb = VB_LIST %]
											[% IF vb.EXT_NUMBER eq param.REDIRECT_TO %]
												<a href='?action=vb_view&id=[% vb.ID %]&uniq=[% USER_CREDS.UNIQ %]' title='edit extension'><img border='0' src='/xvb/images/vb_edit.png' alt='edit' /></a>
											[% END %]
											[% END %]
										</td>
									</tr>
								</table>
							</td>
							</form>
						</tr>
						[% END %]
						<tr align='center' class='nocolor'>
							<form method="post" action="[% CGI_URL %]" onSubmit='return checkChanges(this)'>
							<input type='hidden' name='action' value='gotoif_add'>
							<input type='hidden' name='id' value='[% VBOX_PREF.ID %]'>
							<input type='hidden' name='uniq' value='[% USER_CREDS.UNIQ %]'>
							<td><input size='5' type='text'  name='priority' value='5'></td>
							<td>
								<select name="func">
									<option selected value="value">value</option>
									<option value="strlen">strlen</option>
								</select> 
							</td>
							<td><input size='15' name='var_name' type='text'></td>
							<td>
								<select name="cond">
									<option value="==">==</option>
									<option value="!=">!=</option>
									<option value="<">&lt;</option>
									<option value=">">&gt;</option>
									<option value="<=">&lt;=</option>
									<option value=">=">&gt;=</option>
									<option value="contain">contain</option>
									<option value="regexp">regexp</option>
								</select> 
							</td>
							<td><input size='15' name='var_value' type='text'></td>
							<td><input size='11' name='redirect_to' type='text'></td>
							<td align='center'>
								<input type='submit' value='add'>
							</td>
							</form>
						</tr>
					</table>
				</td>
			</tr>
			[% END %]

		</table>
		</p>
	<p>
	<table width='95%' class='bottom_hr'>
		<tr align='center'>
			<form method="post" action="[% CGI_URL %]" onSubmit='return checkChanges(this)'>
			<td align='center'>
				<input name='action' value='vb_add' type='hidden'>
				<input name='id' value='[% VBOX_PREF.ID %]' type='hidden'>
				<input name='uniq' value='[% USER_CREDS.UNIQ %]' type='hidden'>
				<input size='6' type='text' name='new_ext'>
				<input value='Create new' type='submit'>
			</td>
			</form>
			<td>
				<a class='headers' href='?action=vb_clone&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>Clone</a>
			</td>
			[% IF USER_CREDS.ROOT eq '0' %]
			<td>
				<a class='headers' href='?action=user_info&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>User prefs</a>
			</td>
			[% END %]
			<td>
				<a class='headers' href='?action=msg_list&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>View messages</a>
			</td>
			<td>
				<a class='headers' href='?action=ext_stat&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>Stats</a>
			</td>
			<td>
				<a class='headers' href='?action=vb_del&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]' onclick="return DelConfirm('en')">Delete extension</a>
			</td>
			<td>
				<a class='headers' href='?action=vb_list&id=[% VBOX_PREF.ID %]&uniq=[% USER_CREDS.UNIQ %]'>Extensions list</a>
			</td>
		</tr>
	</table>
	</p>
	<div id='shadow'><div id='loading'></div></div><div id='center'></div>
	</center>
	<script language='JavaScript'>MoreExtOptions(document.getElementById('xvb_view_mode_selector'),1,'all options','core options');</script>
</body>
</html>
